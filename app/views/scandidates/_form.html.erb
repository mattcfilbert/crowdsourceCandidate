<%= form_for ([@senate, @scandidate]) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :party %>
  <%= select(:candidate, :party, ["Democrat", "Republican", "Independent", "Libertarian", "Green", "Other"])%>

  <%= f.label :slogan %>
  <%= f.text_field :slogan %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.label :about %>
  <%= f.text_area :about %>

  <%= f.submit %>

<% end %>
